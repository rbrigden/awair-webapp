<div class="page-header">
	<h2><%= @locale.city %>, <%= @locale.state %></h2>
</div>

<div class="row">
	<div class="col-md-6 col-md-offset-3"> 
		<% @locale.messages.order('created_at DESC').each do |message| %>
		<div class="row">
			<%= render message %>
		</div>
		<% end %>
	</div>

</div>

<div data="<%= @locale.messages.order('created_at DESC').first.created_at %>">
